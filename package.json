{
  "name": "matt kerk - next,prisma,turborepo,postgres starter",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest",
    "turbo-linux-arm64": "latest"
  },
  "engines": {
    "node": "lts"
  },
  "packageManager": "pnpm@7.18.2",
  "pnpm": {
    "peerDependenciesMeta": {
      "@babel/core@>=7.11.0": {
        "optional": true
      }
    }
  }
}